{
	"For Loop": {
		"prefix": "p:for",
		"body": [
			"for (var ${index} = 0; ${index} < ${array}.length; ${index}++) {",
			"\tvar ${element} = ${array}[${index}];",
			"\t$0",
			"}"
		],
		"description": "For Loop"
	},

